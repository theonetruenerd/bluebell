<category name="Bluebell" colour="#a2a2d0" css-icon="customIcon fa-solid fa-flask">
    <label text="Bluebell"/>
    <block type="import_bluebell" />
    <block type="aspirate" />
    <block type="dispense" />
    <block type="distribute_liquid">
        <value name="parameters">
            <shadow type="parameters">
                <mutation color="#4a4b5f" items="2" limit="2" inputType="string" />
                <value name="volume">
                    <shadow type="inline_string">
                        <field name="value">Volume</field>
                    </shadow>
                </value>
                <value name="source">
                    <shadow type="inline_string">
                        <field name="value">Source</field>
                    </shadow>
                </value>
                <value name="destination">
                    <shadow type="inline_string">
                        <field name="value">Destination</field>
                    </shadow>
                </value>
                <value name="liquid_class">
                    <shadow type="inline_string">
                        <field name="value">Liquid Class</field>
                    </shadow>
                </value>
            </shadow>
        </value>
    </block>
</category>