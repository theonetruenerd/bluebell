<category name="Bluebell" colour="#a2a2d0" css-icon="customIcon fa-solid fa-flask">
    <label text="Bluebell"/>
    <block type="import_bluebell" />
    <block type="aspirate" />
    <block type="dispense" />

    <block type="distribute_liquid">
        <value name="parameters">
            <shadow type="parameters">
                <mutation color="#4a4b5f" items="4" limit="4" inputType="string" />
                <value name="input_1">
                    <block type="inline_string">
                        <field name="volume">Volume</field>
                    </block>
                </value>
                <value name="input_2">
                    <shadow type="inline_string">
                        <field name="source">Liquid Source</field>
                    </shadow>
                </value>
                <value name="input_3">
                    <shadow type="inline_string">
                        <field name="destination">Liquid Destination</field>
                    </shadow>
                </value>
                <value name="input_4">
                    <shadow type="inline_string">
                        <field name="liquid_class">Liquid Class</field>
                    </shadow>
                </value>
            </shadow>
        </value>
    </block>
</category>